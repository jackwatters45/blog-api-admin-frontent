{
  "name": "my-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier \"src/**/*.{js,jsx,ts,tsx,css,scss,md,html,json,yml}\" --write",
    "fix": "npm run lint:fix && npm run format",
    "compile": "tsc",
    "test": "jest",
    "preview": "vite preview",
    "prepare": "husky install",
    "rome": "rome format .",
    "rome:fix": "rome format . --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,cjs}": "npm run fix",
    "*.{md,html,css,json,yml}": "prettier --write"
  },
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@jackwatters/simple-nav": "^1.1.2",
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@mui/icons-material": "^5.11.16",
    "@tinymce/tinymce-react": "^4.3.0",
    "@types/react-paginate": "^7.1.1",
    "dotenv": "^16.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-hook-modal-pure": "^1.0.6",
    "react-paginate": "^8.2.0",
    "react-router-dom": "^6.11.1",
    "styled-components": "^6.0.0-rc.1"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "dompurify": "^3.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "html-react-parser": "^3.0.16",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.2",
    "vite": "^4.3.0"
  }
}
